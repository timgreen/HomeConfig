{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ]
    }
  },
  "general": {
    "preferredEditor": "vim",
    "vimMode": true,
    "previewFeatures": true,
    "enablePromptCompletion": true
  },
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "output": {
    "format": "text"
  },
  "ui": {
    "footer": {
      "hideContextPercentage": false
    },
    "showCitations": true
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  }
}
